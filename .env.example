# PuzzlyNest Environment Variables
# Copy this file to .env.local and fill in your values

# Google Analytics 4
# Get from: https://analytics.google.com/ > Admin > Data Streams > Measurement ID
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# Google Search Console Verification
# Get from: https://search.google.com/search-console > Settings > Ownership verification
NEXT_PUBLIC_SEARCH_CONSOLE_VERIFICATION=

# Google AdSense
# Get from: https://www.google.com/adsense > Account > Account Information > Publisher ID
NEXT_PUBLIC_ADSENSE_CLIENT_ID=ca-pub-XXXXXXXXXXXXXXXXXX

# Ad Unit IDs (create in AdSense after approval)
NEXT_PUBLIC_AD_BANNER_TOP=
NEXT_PUBLIC_AD_BANNER_BOTTOM=
NEXT_PUBLIC_AD_SIDEBAR=
NEXT_PUBLIC_AD_IN_GAME=
